package finalArea.initialization;

public class Main {
	// Blank final variable
	final int a;
	static final int b;
	final String c;
	// Instance Initialization Block
	{
		a = 5;
	}

	// Static Initialization Block
	static {
		b = 10;
	}
	
	Main(){
		c="Debasish";
	}

	public static void main(String[] args) {
         Main Obj=new Main();
         System.out.println("a is :" + t.SPEED);
         System.out.println("b is :" + marks);
         System.out.println("c is :" + t.name);
         final int d;
         d = 90;
         System.out.println("Score is :" + d);
         
	}

}
