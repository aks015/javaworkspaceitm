package access_specifier.singleinheritance;

public class Parent {
	//ACCESS SPECIFIER
		private int x = 10;
		        int y = 20; //default
		protected int z = 30; //Child class can use this variable
		public int k = 40; //Child class can use this variable
		
		public void f1()
		{
			System.out.println("INSIDE PARENT F1() METHOD" );
			System.out.println("Private - "+ x);
			System.out.println("Default - "+ y);
			System.out.println("Protected - "+ z);
			System.out.println("Public - "+ k);
			System.out.println();
		}
}
