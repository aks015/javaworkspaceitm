package staticArea;

public class StaticDataInsert {

	static String studentName;
	static int age;

	public static void displayStudentDetails() {
		StaticDataInsert.studentName = "Dev";
		StaticDataInsert.age = 22;
		System.out.println("Student Name : " + studentName + " Age : " + age);
	}

	public static void main(String args[]) {
		StaticDataInsert.displayStudentDetails();
	}

}
